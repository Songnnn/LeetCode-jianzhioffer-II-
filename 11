//JAVA
class Solution {
    public int minArray(int[] numbers) {
        /*
        å·æ‡’
        Arrays.sort(numbers);
        return numbers[0];
        */
        for( int i=0 ; i<numbers.length-1 ; i++ ){
            if( numbers[i]>numbers[i+1] ) return numbers[i+1];
        }
        return numbers[0];
    }
}
